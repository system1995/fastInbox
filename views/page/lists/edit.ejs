
<div class="container" id="page-content">

  <div class="col-md-12">
    <h4>Edit List</h4>
  </div>

  <form action="/lists/update?id=<%= model.id %>" method="POST">
    <div class="form-group">
      <label for="exampleInputEmail1">Name </label>
      <input type="text" class="form-control" id="name"  name="name" placeholder="Enter name" value="<%= model.name  %>">
      <small id="emailHelp" class="form-text text-muted">Name of the list</small>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
  </form>
</div>

