<div class="m-grid__item m-grid__item--fluid m-wrapper">
  <!-- BEGIN: Subheader -->
  <div class="m-subheader ">
    <div class="d-flex align-items-center">
      <div class="mr-auto">
        <h3 class="m-subheader__title ">
          Process invalid emails
        </h3>
      </div>
      <div>
      </div>
    </div>
  </div>
  <!-- END: Subheader -->
  <div class="m-content">
    <!--Begin::Section-->
    <div class="row">
      <div class="col-lg-12">
        <!--Begin::Main Portlet-->
        <div class="m-portlet m-portlet--full-height">
          <!--begin: Portlet Head-->
          <div class="m-portlet__head">
            <div class="m-portlet__head-caption">
              <div class="m-portlet__head-title">
                <h3 class="m-portlet__head-text">
                  <i class="flaticon-refresh"></i> Clean your Email Lists
                </h3>
              </div>
            </div>
            <div class="m-portlet__head-tools">
              <ul class="m-portlet__nav">
                <li class="m-portlet__nav-item">
                  <a href="#" data-toggle="m-tooltip" class="m-portlet__nav-link m-portlet__nav-link--icon" data-direction="left" data-width="auto" title="Get help with filling up this form">
                    <i class="flaticon-info m--icon-font-size-lg3"></i>
                  </a>
                </li>
              </ul>
            </div>
          </div>
          <!--end: Portlet Head-->
          <!--begin: Form Wizard-->
          <div class="m-wizard m-wizard--2 m-wizard--success" id="m_wizard">
            <!--begin: Message container -->
            <div class="m-portlet__padding-x">
              <!-- Here you can put a message or alert -->
            </div>
            <!--end: Message container -->
            <!--begin: Form Wizard Head -->
            <div class="m-wizard__head m-portlet__padding-x">
              <!--begin: Form Wizard Progress -->
              <div class="m-wizard__progress">
                <div class="progress">
                  <div class="progress-bar" role="progressbar"  aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
              </div>
              <!--end: Form Wizard Progress -->
              <!--begin: Form Wizard Nav -->
              <div class="m-wizard__nav">
                <div class="m-wizard__steps">
                  <div class="m-wizard__step"  data-wizard-target="#m_wizard_form_step_1">
                    <a href="#"  class="m-wizard__step-number">
													<span>
														<i class="flaticon-list-3"></i>
													</span>
                    </a>
                    <div class="m-wizard__step-info">
                      <div class="m-wizard__step-title">
                        1. List of subscribers
                      </div>
                      <div class="m-wizard__step-desc">
                        Subscriber to process
                      </div>
                    </div>
                  </div>
                  <div class="m-wizard__step  m-wizard__step--current" data-wizard-target="#m_wizard_form_step_2">
                    <a href="#" class="m-wizard__step-number">
													<span style="background-color:#564ec0">
														<i class="la la-ban"></i>
													</span>
                    </a>
                    <div class="m-wizard__step-info">
                      <div class="m-wizard__step-title">
                        2. Find invalid emails
                      </div>
                      <div class="m-wizard__step-desc">
                        Select emails to delete
                      </div>
                    </div>
                  </div>
                  <div class="m-wizard__step" data-wizard-target="#m_wizard_form_step_3">
                    <a href="#" class="m-wizard__step-number">
													<span>
														<i class="fa  fa-check"></i>
													</span>
                    </a>
                    <div class="m-wizard__step-info">
                      <div class="m-wizard__step-title">
                        3. Confirmation
                      </div>
                      <div class="m-wizard__step-desc">
                        Validation of the process
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <!--end: Form Wizard Nav -->
            </div>
            <!--end: Form Wizard Head -->
            <!--begin: Form Wizard Form-->
            <div class="m-wizard__form">
              <!--
            1) Use m-form--label-align-left class to alight the form input lables to the right
            2) Use m-form--state class to highlight input control borders on form validation
            -->
              <form class="m-form m-form--label-align-left- m-form--state-" id="m_form_import" action="/process_invalid_emails" method="POST" enctype="multipart/form-data">
                <!--begin: Form Body -->
                <input type="hidden" name="current_step" value="2">
                <div class="m-portlet__body">
                  <table class="m-datatable" id="html_table" width="100%">
                    <thead>
                    <tr>
                      <th title="Field #1">
                        #
                      </th>
                      <th title="Field #2">
                        Full Name
                      </th>
                      <th title="Field #3">
                        Email
                      </th>
                      <th title="Field #4">
                        Phone Number
                      </th>
                      <th title="Field #5">
                        Reason
                      </th>
                      <th title="Field #6">
                        Delete ?
                      </th>
                    </tr>
                    </thead>
                    <tbody>
                    <% _.each(subscribers, function (subscriber,i) { %>
                    <tr>
                      <td>
                        <%= i+1 %>
                      </td>
                      <td>
                        <%= subscriber.fullName %>
                      </td>
                      <td>
                        <%= subscriber.email %>
                      </td>
                      <td>
                        <%= subscriber.phoneNumber %>
                      </td>
                      <td>
                        <%= subscriber.reason %>
                      </td>
                      <td>
                        <label class="m-checkbox m-checkbox--square">
                          <input type="checkbox" name="<%= subscriber.id %>">
                          Checked
                          <span></span>
                        </label>
                      </td>
                    </tr>
                    <% }) %>
                    <!--end: Datatable -->
                    </tbody>
                  </table>
                </div>
                <!--end: Form Body -->
                <!--begin: Form Actions -->
                <div class="m-portlet__foot m-portlet__foot--fit">
                  <div class="m-form__actions">
                    <button type="reset" class="btn btn-secondary">
                      Cancel
                    </button>
                    <button href="#" type="submit" class="btn m-btn m-btn--custom m-btn--icon btn-brand" data-wizard-action="next">
															<span>
																<span>
																	Save &amp; Continue
																</span>
																&nbsp;&nbsp;
																<i class="la la-arrow-right"></i>
															</span>
                    </button>
                  </div>
                </div>

                <!--end: Form Actions -->
              </form>
            </div>
            <!--end: Form Wizard Form-->
          </div>
          <!--end: Form Wizard-->
        </div>
        <!--End::Main Portlet-->
      </div>
    </div>
    <!--End::Section-->
  </div>
</div>
<% block('localScripts', '<script src="/js/procInvEmail/new-step2.js" type="text/javascript"></script>') %>




