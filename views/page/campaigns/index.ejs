                      <i class="la la-edit"></i>
                    </a>
                  </span>
                    </td>
                  </tr>

                  <% _.each(campaignsEmail, function (campaign,i) { %>
                  <tr>
                    <td>
                      <%= i+1 %>
                    </td>
                    <td>
                      <%= campaign.name %>
                    </td>
                    <td>
                      <%= campaign.object %>
                    </td>
                    <td>
                      <%= campaign.owner %>
                    </td>
                    <td>
                      <%= campaign.status %>
                    </td>
                    <td>
                      <%=campaign.createdAt.getFullYear()+'-'+campaign.createdAt.getMonth()+'-'+campaign.createdAt.getDate()+" "+
                      campaign.createdAt.getHours()+':'+campaign.createdAt.getMinutes()+':'+campaign.createdAt.getSeconds()+" "
                      %>
                    </td>
                    <td>
                  <span style="overflow: visible; width: 110px;">
                    <div class="dropdown ">
                      <a href="#" class="btn m-btn m-btn--hover-accent m-btn--icon m-btn--icon-only m-btn--pill" data-toggle="dropdown">
                        <i class="la la-ellipsis-h"></i>
                      </a>
                      <div class="dropdown-menu dropdown-menu-right">
                        <a class="dropdown-item" href="/campaigns/edit/?id=<%= campaign.id %>"> <i class="la la-edit"></i> Edit</a>
                        <a class="dropdown-item deleteModal" href="#" data-toggle="modal" data-target="#m_modal_5" deleteId="<%= campaign.id %>"><i class="la la-trash"></i>Delete</a>
                      </div>
                    </div>
                    <a href="#" class="m-portlet__nav-link btn m-btn m-btn--hover-accent m-btn--icon m-btn--icon-only m-btn--pill" title="View ">
                      <i class="la la-edit"></i>
                    </a>
                  </span>
                    </td>
                  </tr>
                  <% }) %>
