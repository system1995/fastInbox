<div class="m-grid__item m-grid__item--fluid m-wrapper">
  <!-- BEGIN: Subheader -->
  <div class="m-subheader ">
    <div class="d-flex align-items-center">
      <div class="mr-auto">
        <h3 class="m-subheader__title ">
          Imports
        </h3>
      </div>
      <div>
      </div>
    </div>
  </div>
  <!-- END: Subheader -->
  <div class="m-content">
    <!--Begin::Section-->
    <div class="row">
      <div class="col-lg-12">
        <!--begin::Portlet-->
        <div class="m-portlet m-portlet--mobile">
          <div class="m-portlet__head">
            <div class="m-portlet__head-caption">
              <div class="m-portlet__head-title">
                <h3 class="m-portlet__head-text">
                  <i class="fa fa-cloud-upload"></i> Imports<small>All your imports</small>
                </h3>
              </div>
            </div>
            <div class="m-portlet__head-tools">
              <ul class="m-portlet__nav">
              </ul>
            </div>
          </div>
          <div class="m-portlet__body">
            <!--begin: Search Form -->
            <div class="m-form m-form--label-align-right m--margin-top-20 m--margin-bottom-30">
              <div class="row align-items-center">
                <div class="col-xl-8 order-2 order-xl-1">
                  <div class="form-group m-form__group row align-items-center">
                    <div class="col-md-4">
                      <div class="m-form__group m-form__group--inline">
                        <div class="m-form__label">
                          <label>
                            Progress:
                          </label>
                        </div>
                        <div class="input-group bootstrap-touchspin">
                          <input id="m_touchspin_1" class="form-control" name="demo1" placeholder="Progress" style="display: block;" type="text">
                        </div>
                      </div>
                      <div class="d-md-none m--margin-bottom-10"></div>
                    </div>
                    <div class="col-md-4">
                      <div class="m-input-icon m-input-icon--left">
                        <input type="text" class="form-control m-input m-input--solid" placeholder="Search..." id="generalSearch">
                        <span class="m-input-icon__icon m-input-icon__icon--left">
															<span>
																<i class="la la-search"></i>
															</span>
														</span>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="col-xl-4 order-1 order-xl-2 m--align-right">
                  <a href="/imports/new" class="btn btn-brand  m-btn--pill m-btn btn btn-brand m-btn m-btn--custom m-btn--bolder">
												<span>
													<i class="flaticon-add-circular-button"></i>
													<span>
														Import subscribers from file
													</span>
												</span>
                  </a>
                  <div class="m-separator m-separator--dashed d-xl-none"></div>
                </div>
              </div>
            </div>
            <!--end: Search Form -->
            <!--begin: Datatable -->
            <table class="m-datatable" id="html_table" width="100%">
              <thead>
              <tr>
                <th title="Field #1">
                  #
                </th>
                <th title="Field #2">
                  Name
                </th>
                <th title="Field #3">
                  Lists
                </th>
                <th title="Field #4">
                  Progress
                </th>
                <th title="Field #5">
                  Created Date
                </th>
                <th title="Field #6">
                  Actions
                </th>
              </tr>
              </thead>
              <tbody>
              <% _.each(imports, function (Import,i) { %>
              <tr>
                <td>
                  <%= i+1 %>
                </td>
                <td>
                  <%= Import.name %>
                </td>
                <td>
                  <% _.each(Import.lists, function (list) { %>
                  <%= list.name %> </br>
                  <% }) %>
                </td>
                <td>
                  <div class="progress">
                    <div class="progress-bar" role="progressbar" style="width: <%= Import.progress %>%;" value="<%= Import.progress %>" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100"><%= Import.progress %>%</div>
                  </div>
                </td>
                <td>
                  <%=Import.createdAt.getFullYear()+'-'+Import.createdAt.getMonth()+'-'+Import.createdAt.getDate()+" "+
                  Import.createdAt.getHours()+':'+Import.createdAt.getMinutes()+':'+Import.createdAt.getSeconds()+" "
                  %>
                </td>
                <td>
                  <span style="overflow: visible; width: 110px;">
                    <div class="dropdown ">
                      <a href="#" class="btn m-btn m-btn--hover-accent m-btn--icon m-btn--icon-only m-btn--pill" data-toggle="dropdown">
                        <i class="la la-ellipsis-h"></i>
                      </a>
                      <div class="dropdown-menu dropdown-menu-right">
                        <a class="dropdown-item" href="/Import/edit/?id=<%= Import.id %>"> <i class="la la-edit"></i> Edit</a>
                        <a class="dropdown-item" href="/Import/destroy/?id=<%= Import.id %>"><i class="la la-trash"></i>Delete</a>
                      </div>
                    </div>
                    <a href="#" class="m-portlet__nav-link btn m-btn m-btn--hover-accent m-btn--icon m-btn--icon-only m-btn--pill" title="View ">
                      <i class="la la-edit"></i>
                    </a>
                  </span>
                </td>
              </tr>
              <% }) %>
              </tbody>
            </table>
            <!--end: Datatable -->
          </div>
        </div>
        <!--end::Portlet-->
      </div>
    </div>
    <!--End::Section-->
  </div>
</div>
<% block('localScripts', '<script src="/js/Imports/index.js" type="text/javascript"></script>') %>
