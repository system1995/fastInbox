
<div class="container" id="page-content">
  <div class="row">
    <div class="col-md-12">
      <h4>Imports </h4>
      <a href="/imports/new"><button type="button" class="btn btn-primary">Import subscribers from file</button></a>
      <div class="table-responsive">
        <table id="mytable" class="table table-bordred table-striped">
          <thead>
          <th><input type="checkbox" id="checkall" /></th>
          <th>Name</th>
          <th>Lists</th>
          <th>Status</th>
          <th>Show</th>
          <th>Delete</th>
          </thead>
          <tbody>

          <% _.each(model, function(Import) { %>
          <tr>
            <td><input type="checkbox" class="checkthis" /></td>
            <td><%= Import.name %></td>
            <td>
            <% _.each(Import.lists, function (list) { %>
              <%= list.name %> </br>
            <% }) %>
            </td>
            <td><div class="progress">
                <div class="progress-bar" role="progressbar" style="width: 70%;" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100">70%</div>
              </div>
            </td>
            <td><a href="/imports/show?id=<%= Import.id %>"><i class="fas fa-search-plus"></i><span>Show</span></a></td>
            <td><a href="/imports/destroy/?id=<%= Import.id %>"><i class="fa fa-trash"></i><span>Delete</span></a></td>
            <% }) %>
          </tr>
          </tbody>
        </table>
        <div class="clearfix"></div>
        <nav aria-label="Page navigation example">
          <ul class="pagination">
            <li class="page-item"><a class="page-link" href="#">Previous</a></li>
            <li class="page-item"><a class="page-link" href="#">1</a></li>
            <li class="page-item"><a class="page-link" href="#">2</a></li>
            <li class="page-item"><a class="page-link" href="#">3</a></li>
            <li class="page-item"><a class="page-link" href="#">Next</a></li>
          </ul>
        </nav>

      </div>

    </div>
  </div>
</div>

